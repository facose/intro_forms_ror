<section class="users container">
    <% @users.each do |user| %>
        <div class="card">
            <div class="card-body">
                <h5 class="card-title"><%= user.name %></h5>
                <p class="card-text"><%= user.email %></p>
            </div>
        </div>
    <% end %>
</section>
<div class="new_register">
<%= link_to 'Otro Registro', '/users/form', :class => 'btn btn-secondary' %>
</div>